{
  "name": "spscript",
  "version": "1.3.1",
  "description": "SharePoint 2013 Rest Api Wrappers",
  "main": "./src/entries/spscript.js",
  "scripts": {
    "prebuild": "babel src -d lib --source-maps --presets es2015 --plugins transform-object-assign --ignore \"src/tests\"",
    "build": "webpack",
    "postbuild": "npm run minify && rimraf dist/v2/plugins.js && rimraf dist/v2/plugins.js.map",
    "docs": "jsdoc -c ./jsdoc/jsdoc.config.json -r readme.md",
    "pretest": "npm run build",
    "test": "node src/tests/run.js",
    "minify": "uglifyjs dist/v2/spscript.js -o dist/v2/spscript.min.js --stats",
    "clean": "rimraf lib",
    "watch": "watch --interval .5 \"npm run build\" ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DroopyTersen/spscript"
  },
  "keywords": [
    "sharepoint"
  ],
  "author": "Andrew Petersen",
  "bugs": {
    "url": "https://github.com/DroopyTersen/spscript/issues"
  },
  "homepage": "https://github.com/DroopyTersen/spscript",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "ink-docstrap": "^1.1.4",
    "ngrok": "^2.1.7",
    "rimraf": "^2.5.2",
    "spbrander": "^1.0.6",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.6.2",
    "watch": "^0.18.0",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "babel-plugin-transform-object-assign": "^6.5.0",
    "chai": "^1.10.0",
    "droopy-templating": "^1.0.2",
    "es6-promise": "^3.1.2",
    "jquery": "^2.1.3",
    "jquery.fancytree": "^2.12.0",
    "mocha": "^2.4.5",
    "select2-browserify": "^3.4.5",
    "typeahead.js": "^0.11.1",
    "zepto": "0.0.4"
  }
}
