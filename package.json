{
  "name": "spscript",
  "version": "4.2.0",
  "description": "ShareP0oint Rest Api Wrappers",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "umd:main": "dist/spscript.js",
  "scripts": {
    "prebuild": "rimraf lib && rimraf .cache && rimraf dist",
    "build-old": "tsc && npm run docs && npm run parcel",
    "build": "rollup -c",
    "parcel": "parcel build src/index.ts --out-file spscript.js",
    "test": "jest --verbose --coverage",
    "test:watch": "jest --verbose --watch",
    "docs": "docsify init",
    "docs:serve": "docsify serve docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DroopyTersen/spscript"
  },
  "keywords": [
    "sharepoint",
    "spfx",
    "o365",
    "microsoft",
    "office"
  ],
  "author": "Andrew Petersen",
  "bugs": {
    "url": "https://github.com/DroopyTersen/spscript/issues"
  },
  "homepage": "https://github.com/DroopyTersen/spscript",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "chai": "^1.10.0",
    "concurrently": "^3.4.0",
    "docsify": "^4.11.3",
    "docsify-cli": "^4.4.0",
    "dotenv": "^6.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.1.0",
    "ngrok": "^2.2.17",
    "node-sp-auth": "^2.5.7",
    "parcel-bundler": "^1.12.2",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^2.6.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
